    <term_definitions language="en">
      <items code="at0000">
        <items id="description">The clinical history of a person, as told to a clinician or recorded directly by an individual/patient.</items>
        <items id="text">Story/History</items>
      </items>
      <items code="at0001">
        <items id="description">@ internal @</items>
        <items id="text">Event Series</items>
      </items>
      <items code="at0002">
        <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
        <items id="text">Any event</items>
      </items>
      <items code="at0003">
        <items id="description">@ internal @</items>
        <items id="text">Tree</items>
      </items>
      <items code="at0004">
        <items id="description">Narrative description of the clinical history or story.</items>
        <items id="text">Story</items>
      </items>
      <items code="at0006">
        <items id="comment">For example: a specific symptom such as nausea or pain; an event such as a fall off a bicycle; or an issue such as a desire to quit using tobacco.</items>
        <items id="description">Additional structured details about the individual's story or patient's history.</items>
        <items id="text">Details</items>
      </items>
    </term_definitions>
  </component_ontologies>
  <component_ontologies archetype_id="openEHR-EHR-SECTION.adhoc.v1">
    <term_definitions language="en">
      <items code="at0000">
        <items id="description">A generic section header which should be renamed in a template to suit a specific clinical context.</items>
        <items id="text">Ad hoc heading</items>
      </items>
    </term_definitions>
  </component_ontologies>
  <component_ontologies archetype_id="openEHR-EHR-OBSERVATION.nutritional_risk_screening.v1">
    <term_definitions language="en">
      <items code="at0000">
        <items id="description">*Screening for malnutrition using the tool NRS 2002.(nb)</items>
        <items id="text">*Nutritional risk screening(nb)</items>
      </items>
      <items code="at0001">
        <items id="description">@ internal @</items>
        <items id="text">Event Series</items>
      </items>
      <items code="at0002">
        <items id="description">*(nb)</items>
        <items id="text">*Any event(nb)</items>
      </items>
      <items code="at0003">
        <items id="description">*@ internal @(nb).(nb)</items>
        <items id="text">*Tree(nb)</items>
      </items>
      <items code="at0004">
        <items id="description">*Er pasientens BMI under 20,5?(nb)</items>
        <items id="text">*Er BMI &lt; 20,5?(nb)</items>
      </items>
      <items code="at0005">
        <items id="description">*Vekttap de siste ukene?(nb)</items>
        <items id="text">*Har pasienten tapt vekt de siste tre månendene?(nb)</items>
      </items>
      <items code="at0006">
        <items id="description">*Redusert ernæringsinntak siste uken?(nb)</items>
        <items id="text">*Har pasienten hatt redusert næringsinntak den siste uken?(nb)</items>
      </items>
      <items code="at0007">
        <items id="description">*Alvorlig sykdom? (Eksempel intensivopphold).(nb)</items>
        <items id="text">*Er pasienten alvorlig syk.(nb)</items>
      </items>
      <items code="at0016">
        <items id="description">*Kartlegging av ernæringstilstand.(nb)</items>
        <items id="text">*Ernæringstilstand(nb)</items>
      </items>
      <items code="at0017">
        <items id="description">*</items>
        <items id="text">Severity of disease.</items>
      </items>
      <items code="at0018">
        <items id="description">*Normal ernæringstilstand.(nb)</items>
        <items id="text">*Normal(nb)</items>
      </items>
      <items code="at0019">
        <items id="description">*Vekttap &gt; 5 % siste 3 mnd. eller  matinntak &lt; 50 – 75 % av behov siste uke.(nb)</items>
        <items id="text">*Mild(nb)</items>
      </items>
      <items code="at0020">
        <items id="description">*Vekttap  &gt; 5 % siste 2 mnd. eller BMI 18.5 – 20.5 og redusert allmenntilstand eller  matinntak 25–50 % av behov sist uke.(nb)</items>
        <items id="text">*Moderat(nb)</items>
      </items>
      <items code="at0021">
        <items id="description">*Vekttap &gt; 5 % siste mnd. eller BMI &lt; 18.5 og redusert allmenntilstand eller  matinntak 0–25 % av behov siste uke.(nb)</items>
        <items id="text">*Alvorlig(nb)</items>
      </items>
      <items code="at0022">
        <items id="description"> Normal nutritional requirements.</items>
        <items id="text">Absent.</items>
      </items>
      <items code="at0023">
        <items id="description">*En pasient med kronisk sykdom eller en pasient som har 
gjennomgått et mindre kirurgisk inngrep.(nb)</items>
        <items id="text">*Mild(nb)</items>
      </items>
      <items code="at0024">
        <items id="description">*En pasient med tydelig redusert allmenntilstand pga sin sykdom.(nb)</items>
        <items id="text">*Moderat(nb)</items>
      </items>
      <items code="at0025">
        <items id="description">*En pasient som er alvorlig syk.(nb)</items>
        <items id="text">*Alvorlig(nb)</items>
      </items>
      <items code="at0027">
        <items id="description">*</items>
        <items id="text">Age 70 and over.</items>
      </items>
      <items code="at0028">
        <items id="description">*Pasienten er yngre enn 70 år.(nb)</items>
        <items id="text">*Nei(nb)</items>
      </items>
      <items code="at0029">
        <items id="description">*Pasienten er 70 år eller eldre.(nb)</items>
        <items id="text">*Ja(nb)</items>
      </items>
      <items code="at0030">
        <items id="description">*Summen av score fra ernæringstilstand, sykdommens alvorlighetsgrad og aldersjustering.(nb)</items>
        <items id="text">*Total aldersjustert score(nb)</items>
      </items>
      <items code="at0031">
        <items id="description">*Dersom svaret er "Ja" på noen av de innledende spørsmålene, gjennomføres hovedscreeningen.(nb)</items>
        <items id="text">*Innledende screening(nb)</items>
      </items>
      <items code="at0032">
        <items id="description">*Den avsluttende screeningen gjennomføres kun dersom det svares "Ja" på noen av spørsmålene i den innledende screeningen.(nb)</items>
        <items id="text">*Hovedscreening(nb)</items>
      </items>
      <items code="at0033">
        <items id="description">*Pasientens BMI er under 20,5.(nb)</items>
        <items id="text">*Ja(nb)</items>
      </items>
      <items code="at0034">
        <items id="description">*Pasientens BMI er over eller lik 20,5.(nb)</items>
        <items id="text">*Nei(nb)</items>
      </items>
      <items code="at0035">
        <items id="description">*Pasienten har tapt vekt i løpet av de 3 siste månedene.(nb)</items>
        <items id="text">*Ja(nb)</items>
      </items>
      <items code="at0036">
        <items id="description">*Pasienten har ikke tapt vekt i løpet av de 3 siste månedene.(nb)</items>
        <items id="text">*Nei(nb)</items>
      </items>
      <items code="at0037">
        <items id="description">*Pasienten har hatt redusert næringsinntak i den siste uken.(nb)</items>
        <items id="text">*Ja(nb)</items>
      </items>
      <items code="at0038">
        <items id="description">*Pasienten har ikke hatt redusert næringsinntak i den siste uken.(nb)</items>
        <items id="text">*Nei(nb)</items>
      </items>
      <items code="at0039">
        <items id="description">*Pasienten er alvorlig/kritisk syk.(nb)</items>
        <items id="text">*Ja(nb)</items>
      </items>
      <items code="at0040">
        <items id="description">*Pasienten er ikke alvorlig/kritisk syk.(nb)</items>
        <items id="text">*Nei(nb)</items>
      </items>
    </term_definitions>
  </component_ontologies>
  <component_ontologies archetype_id="openEHR-EHR-OBSERVATION.madrs_no.v1">
    <term_definitions language="nb">
      <items code="at0000">
        <items id="description">MADRS (Montgomery and Åsberg Depression Rating Scale) brukes for å måle dybden av en depressiv tilstand og som hjelpemiddel for å følge opp en behandling.</items>
        <items id="text">MADRS</items>
      </items>
      <items code="at0001">
        <items id="description">@ internal @</items>
        <items id="text">Event Series</items>
      </items>
      <items code="at0002">
        <items id="description">Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i en template eller i en applikasjon.</items>
        <items id="text">Uspesifisert hendelse</items>
      </items>
      <items code="at0003">
        <items id="description">@ internal @</items>
        <items id="text">Tree</items>
      </items>
      <items code="at0004">
        <items id="comment">Fylles ut når intervjuet er over, på grunnlag av observasjon.</items>
        <items id="description">Sikter på dysterhet, tungsinn, fortvilelse (mer enn bare vanlig forbigående dårlig humør) slik det avspeiler seg i tale, mimikk og holdning. Gradér på grunnlag av dybde og manglende evne til å lysne opp.</items>
        <items id="text">Synlig tristhet</items>
      </items>
      <items code="at0005">
        <items id="description">Tar sikte på subjektivt opplevet sinnsstemning, uansett om stemningen gir seg ytre uttrykk eller ikke. Omfatter senket stemningsleie, tristhet, oppgitthet, tungsinn, håpløshet og hjelpeløshet. Gradér i henhold til intensitet, varighet og i hvilken grad sinnsstemningen påvirkes av ytre omstendigheter. Oppstemthet skåres som 0.</items>
        <items id="text">Tristhet</items>
      </items>
      <items code="at0006">
        <items id="description">Sikter på følelse av vag ulystfølelse, ubehagelig indre spenning, uro, angst, stigende til panikk. Gradér i henhold til intensitet, varighet og behov for hjelp og støtte. Skilles fra "Tristhet".</items>
        <items id="text">Indre spenning</items>
      </items>
      <items code="at0007">
        <items id="description">Sikter på subjektiv opplevelse av kortvarig eller mindre dyp søvn sammenlignet med vanlig tilstand.</items>
        <items id="text">Redusert nattesøvn</items>
      </items>
      <items code="at0008">
        <items id="description">Sikter på følelsen av at appetitten er svekket i forhold til tidligere.</items>
        <items id="text">Svekket appetitt</items>
      </items>
      <items code="at0009">
        <items id="description">Sikter på vansker med å samle tankene, stigende til invalidiserende mangel på konsentrasjonsevne. Gradér i henhold til intensitet, frekvens og grad av invalidiserende effekt.</items>
        <items id="text">Konsentrasjonsvansker</items>
      </items>
      <items code="at0010">
        <items id="description">Sikter på vanskelighet med å komme i gang, treghet med å ta initiativ og å utføre dagligdagse aktiviteter.</items>
        <items id="text">Initiativløshet</items>
      </items>
      <items code="at0011">
        <items id="description">Sikter på den subjektive opplevelse av svekkede interesser for omgivelsene, eller for aktiviteter som normalt gir glede. Evnen til å reagere med adekvate følelser på omstendighetene eller andre mennesker er redusert. Skilles fra "Initiativløshet".</items>
        <items id="text">Svekkede følelsesmessige reaksjoner</items>
      </items>
      <items code="at0012">
        <items id="description">Sikter på tanker om skyld, mindreverdighet, selvbebreidelser, anger og tanker om økonomisk ruin, etc.</items>
        <items id="text">Depressivt tankeinnhold</items>
      </items>
      <items code="at0013">
        <items id="description">Tar sikte på følelsen av at livet ikke er verd å leve, at en naturlig død ville være velkommen, suicidale tanker og forberedelse til selvmord.</items>
        <items id="text">Suicidaltanker</items>
      </items>
      <items code="at0014">
        <items id="description">*</items>
        <items id="text">Stort sett indifferent stemningsleie. Leilighetsvis tristhet når omstendighetene tilsier det</items>
      </items>
      <items code="at0015">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0016">
        <items id="description">*</items>
        <items id="text">Ingen tristhet</items>
      </items>
      <items code="at0017">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0018">
        <items id="description">*</items>
        <items id="text">Ser trist og humørløs ut, men kan leilighetsvis lysne opp</items>
      </items>
      <items code="at0019">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0020">
        <items id="description">*</items>
        <items id="text">Virker trist og ulykkelig hele tiden</items>
      </items>
      <items code="at0021">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0022">
        <items id="description">*</items>
        <items id="text">Ekstrem og vedvarende tristhet og fortvilelse</items>
      </items>
      <items code="at0023">
        <items id="description">*</items>
        <items id="text">Overveiende følelse av nedstemthet, men lysere øyeblikk forekommer</items>
      </items>
      <items code="at0024">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0025">
        <items id="description">*</items>
        <items id="text">Gjennomtrengende følelse av tristhet og dysterhet. Sinnsstemningen influeres knapt av ytre omstendigheter</items>
      </items>
      <items code="at0026">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0027">
        <items id="description">*</items>
        <items id="text">Konstant opplevelse av sterk tristhet og fortvilelse</items>
      </items>
      <items code="at0028">
        <items id="description">*</items>
        <items id="text">Rolig, bare antydning til indre spenning</items>
      </items>
      <items code="at0029">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0030">
        <items id="description">*</items>
        <items id="text">Leilighetsvis følelse av ubehagelig psykisk spenning og vag uro</items>
      </items>
      <items code="at0031">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0032">
        <items id="description">*</items>
        <items id="text">Vedvarende følelse av indre spenning eller panikkepisoder som pasienten bare kan mestre med vanskelighet</items>
      </items>
      <items code="at0033">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0034">
        <items id="description">*</items>
        <items id="text">Vedvarende frykt og angst. Overveldende panikkfølelse</items>
      </items>
      <items code="at0035">
        <items id="description">*</items>
        <items id="text">Sover som vanlig</items>
      </items>
      <items code="at0036">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0037">
        <items id="description">*</items>
        <items id="text">Moderate innsovningsvansker eller moderat redusert, lett eller urolig søvn</items>
      </items>
      <items code="at0038">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0039">
        <items id="description">*</items>
        <items id="text">Søvnen redusert eller avbrutt med minst to timer sammenlignet med vanlig</items>
      </items>
      <items code="at0040">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0041">
        <items id="description">*</items>
        <items id="text">Mindre enn to til tre timers søvn om natten</items>
      </items>
      <items code="at0042">
        <items id="description">*</items>
        <items id="text">Normal eller økt appetitt</items>
      </items>
      <items code="at0043">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0044">
        <items id="description">*</items>
        <items id="text">Moderat redusert appetitt</items>
      </items>
      <items code="at0045">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0046">
        <items id="description">*</items>
        <items id="text">Nesten ingen appetitt, maten smaker ikke, må tvinge seg selv til å spise</items>
      </items>
      <items code="at0047">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0048">
        <items id="description">*</items>
        <items id="text">Næringsvegrende. Må overtales av andre til å spise</items>
      </items>
      <items code="at0049">
        <items id="description">*</items>
        <items id="text">Ingen konsentrasjonsvansker</items>
      </items>
      <items code="at0050">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0051">
        <items id="description">*</items>
        <items id="text">Leilighetsvis vansker med å samle tankene</items>
      </items>
      <items code="at0052">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0053">
        <items id="description">*</items>
        <items id="text">Vansker med konsentrasjon og vedvarende oppmerksomhet, som forstyrrer lesing eller konversasjon</items>
      </items>
      <items code="at0054">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0055">
        <items id="description">*</items>
        <items id="text">Invalidiserende mangel på konsentrasjonsevne</items>
      </items>
      <items code="at0056">
        <items id="description">*</items>
        <items id="text">Ingen spesielle vansker med å komme i gang med ting, ingen treghet</items>
      </items>
      <items code="at0057">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0058">
        <items id="description">*</items>
        <items id="text">Visse vansker med å komme i gang med aktiviteter</items>
      </items>
      <items code="at0059">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0060">
        <items id="description">*</items>
        <items id="text">Vanskeligheter med å komme i gang med enkle rutineaktiviteter, som kan utføres bare med betydelig anstrengelse</items>
      </items>
      <items code="at0061">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0062">
        <items id="description">*</items>
        <items id="text">Helt manglende evne til å ta initiativ. Uten evne til å igangsette aktiviteter uten hjelp</items>
      </items>
      <items code="at0063">
        <items id="description">*</items>
        <items id="text">Normal interesse for omgivelsene og andre mennesker</items>
      </items>
      <items code="at0064">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0065">
        <items id="description">*</items>
        <items id="text">Redusert evne til å glede seg over ting som vanligvis gleder. Redusert evne til å føle sinne</items>
      </items>
      <items code="at0066">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0067">
        <items id="description">*</items>
        <items id="text">Tap av interesse for omverdenen. Tap av følelser for venner og bekjente</items>
      </items>
      <items code="at0068">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0069">
        <items id="description">*</items>
        <items id="text">Opplevelsen av å være følelsesmessig lammet, uten evne til å føle sinne eller sorg, og til dels smertefull opplevelse av ikke å kunne føle noe for nære slektninger eller venner</items>
      </items>
      <items code="at0070">
        <items id="description">*</items>
        <items id="text">Ingen depressive tanker</items>
      </items>
      <items code="at0071">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0072">
        <items id="description">*</items>
        <items id="text">Fluktuerende tanker om ikke å strekke til, selvbebreidelser og mindreverdighetsfølelse</items>
      </items>
      <items code="at0073">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0074">
        <items id="description">*</items>
        <items id="text">Vedvarende selvbebreidelser eller klare, men fortsatt rasjonelle tanker om skyld og synd. Ser uttalt pessimistisk på fremtiden.</items>
      </items>
      <items code="at0075">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0076">
        <items id="description">*</items>
        <items id="text">Vrangforestillinger om økonomisk ruin eller utilgivelige synder. Absurde selvanklager.</items>
      </items>
      <items code="at0077">
        <items id="description">*</items>
        <items id="text">Ordinær livslyst, tar det som det kommer</items>
      </items>
      <items code="at0078">
        <items id="description">*</items>
        <items id="text">Mellom 0 og 2</items>
      </items>
      <items code="at0079">
        <items id="description">*</items>
        <items id="text">Lei av livet, men bare flyktige suicidaltanker</items>
      </items>
      <items code="at0080">
        <items id="description">*</items>
        <items id="text">Mellom 2 og 4</items>
      </items>
      <items code="at0081">
        <items id="description">*</items>
        <items id="text">Ville helst være død, suicidaltanker opptrer hyppig, og suicid anses som en mulig løsning uten at det foreligger spesifikke planer eller intensjoner</items>
      </items>
      <items code="at0082">
        <items id="description">*</items>
        <items id="text">Mellom 4 og 6</items>
      </items>
      <items code="at0083">
        <items id="description">*</items>
        <items id="text">Uttalte planer om suicid når anledningen byr seg. Aktive forberedelser for suicid</items>
      </items>
      <items code="at0084">
        <items id="comment">Det finnes tolkningsveiledere for MADRS, der man ut fra MADRS skår havner på ingen depresjon, lett, moderat eller alvorlig.</items>
        <items id="description">Summen av skår fra hvert enkelt MADRS-parameter.</items>
        <items id="text">MADRS skår</items>
