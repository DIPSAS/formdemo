            <term_definitions code="at0000">
              <items id="description">Vurderingsverktøy for identifisering av fallrisiko hos eldre pasienter på sykehus eller andre heldøgnsinstitusjoner. Denne versjonen er basert på den modifiserte norske oversettelsen som er tatt i bruk av det nasjonale pasientsikkerhetsprogrammet.</items>
              <items id="text">STRATIFY</items>
            </term_definitions>
            <term_definitions code="at0001">
              <items id="description">@ internal @</items>
              <items id="text">Event Series</items>
            </term_definitions>
            <term_definitions code="at0002">
              <items id="description">Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i en template eller i en applikasjon.</items>
              <items id="text">Uspesifisert hendelse</items>
            </term_definitions>
            <term_definitions code="at0003">
              <items id="description">@ internal @</items>
              <items id="text">Tree</items>
            </term_definitions>
            <term_definitions code="at0004">
              <items id="comment">score 0 for Usann/ Nei, 
score 1 for Sann/ Ja.</items>
              <items id="description">Har pasient hatt noen fall i de siste 3 månedene?</items>
              <items id="text">Fall de siste tre måneder</items>
            </term_definitions>
            <term_definitions code="at0005">
              <items id="comment">score 0 for Usann/ Nei, 
score 1 for Sann/ Ja.</items>
              <items id="description">Er pasient urolig?</items>
              <items id="text">Urolig atferd</items>
            </term_definitions>
            <term_definitions code="at0006">
              <items id="comment">score 0 for Usann/ Nei, 
score 1 for Sann/ Ja.</items>
              <items id="description">Er pasient synsskadet/synshemmet i et slikt omfang at hverdagslig funksjoner er påvirket?</items>
              <items id="text">Synshemminger</items>
            </term_definitions>
            <term_definitions code="at0007">
              <items id="comment">score 0 for Usann/ Nei, 
score 1 for Sann/ Ja.</items>
              <items id="description">Oppfatter du at pasient har spesielt behov av å besøke toalett ofte?</items>
              <items id="text">Hyppige toalettbesøk</items>
            </term_definitions>
            <term_definitions code="at0009">
              <items id="comment">0 = lav risko, 
1 = moderat risko, 
&gt;2 = høy risko.</items>
              <items id="description">Sum av poeng fra alle elementene.</items>
              <items id="text">Poeng totalt</items>
            </term_definitions>
            <term_definitions code="at0011">
              <items id="description">Pasienten har ikke hatt noen fall siste 3 måneder.</items>
              <items id="text">Nei</items>
            </term_definitions>
            <term_definitions code="at0012">
              <items id="description">Pasienten har hatt fall siste 3 måneder.</items>
              <items id="text">Ja</items>
            </term_definitions>
            <term_definitions code="at0013">
              <items id="description">Pasienten viser ikke urolig atferd.</items>
              <items id="text">Nei</items>
            </term_definitions>
            <term_definitions code="at0014">
              <items id="description">Pasienten viser urolig atferd.</items>
              <items id="text">Ja</items>
            </term_definitions>
            <term_definitions code="at0015">
              <items id="description">Pasienten har ikke store synshemminger.</items>
              <items id="text">Nei</items>
            </term_definitions>
            <term_definitions code="at0017">
