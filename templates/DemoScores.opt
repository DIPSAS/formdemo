  <component_ontologies archetype_id="openEHR-EHR-OBSERVATION.barthel_no.v1">
    <term_definitions language="nb">
      <items code="at0000">
        <items id="comment">Barthel ADL-indeks er opprinnelig oversatt til norsk ved Knut Laake. Denne norske versjonen er redigert i 2008 av Ingvild Saltvedt, Jorunn L. Helbostad, Unni Sveen, Pernille Thingstad, Olav Sletvold og Torgeir Bruun Wyller på grunnlag av flere tidligere norske oversettelser og med hovedvekt på originalpublikasjonen fra 1965.</items>
        <items id="description">Barthel ADL-indeks er en sjekkliste for selvstendighet i basale aktiviteter i dagliglivet (ADL).</items>
        <items id="text">Barthel ADL-indeks</items>
      </items>
      <items code="at0001">
        <items id="description">@ internal @</items>
        <items id="text">List</items>
      </items>
      <items code="at0002">
        <items id="description">@ internal @</items>
        <items id="text">History</items>
      </items>
      <items code="at0003">
        <items id="description">Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i en template eller i en applikasjon.</items>
        <items id="text">Uspesifisert hendelse</items>
      </items>
      <items code="at0004">
        <items id="description">Evne til å kontrollere vannlating.</items>
        <items id="text">Blærekontroll</items>
      </items>
      <items code="at0005">
        <items id="description">Helt inkontinent eller trenger permanent kateter.</items>
        <items id="text">Helt inkontinent</items>
      </items>
      <items code="at0006">
        <items id="description">Nedsatt kontroll og enkelte "uhell" eller holder seg tørr med uridom eller kateter, men trenger hjelp for å bruke dette.</items>
        <items id="text">Nedsatt kontroll</items>
      </items>
      <items code="at0007">
        <items id="description">Holder seg evt. tørr ved bruk av uridom 
eller mestrer bruk av kateter på egen hånd.</items>
        <items id="text">Kontinent</items>
      </items>
      <items code="at0008">
        <items id="description">Evne til å kontrollere avføring.</items>
        <items id="text">Tarmkontroll</items>
      </items>
      <items code="at0009">
        <items id="description">Evne til å bade eller dusje på egen hånd.</items>
        <items id="text">Bading/dusj</items>
      </items>
      <items code="at0010">
        <items id="description">Trenger hjelp.</items>
        <items id="text">Trenger hjelp</items>
      </items>
      <items code="at0011">
        <items id="description">Helt selvhjulpen.</items>
        <items id="text">Selvhjulpen</items>
      </items>
      <items code="at0012">
        <items id="description">Evne til å velge og ta på seg klær.</items>
        <items id="text">Påkledning</items>
      </items>
      <items code="at0013">
        <items id="description">Trenger hjelp til mer enn halvparten ved påkledning.</items>
        <items id="text">Trenger hjelp til mer enn halvparten</items>
      </items>
      <items code="at0014">
        <items id="description">Trenger hjelp, men klarer halvparten innen rimelig tid.</items>
        <items id="text">Trenger hjelp, men klarer halvparten innen rimelig tid</items>
      </items>
      <items code="at0016">
        <items id="comment">Dette feltet skal ikke brukes dersom man kun benytter utvalgte felt i arketypen.</items>
        <items id="description">Summen av de 10 elementene i Barthel ADL-indeks. Tall på 0-9 indikerer høyt hjelpebehov, 10-19 moderat hjelpebehov og 20 er selvhjulpen.</items>
        <items id="text">Sumskår</items>
      </items>
      <items code="at0017">
        <items id="description">Evne til å forflytte seg til fots eller i rullestol.</items>
        <items id="text">Mobilitet</items>
      </items>
      <items code="at0018">
        <items id="description">Kan ikke kjøre rullestol uten hjelp.</items>
        <items id="text">Kan ikke kjøre rullestol uten hjelp</items>
      </items>
      <items code="at0019">
        <items id="description">Kan ikke gå, men kan kjøre rullestol uten hjelp 50 meter.</items>
        <items id="text">Kan ikke gå</items>
      </items>
      <items code="at0020">
        <items id="description">Kan gå 50 meter med rullator og/eller støtte/tilsyn av en person.</items>
        <items id="text">Kan gå med rullator og/eller støtte/tilsyn</items>
      </items>
      <items code="at0021">
        <items id="description">Evne til forflytning mellom seng og stol.</items>
        <items id="text">Forflytning mellom seng og stol</items>
      </items>
      <items code="at0022">
        <items id="description">Sengeliggende.</items>
        <items id="text">Kan ikke sitte</items>
      </items>
      <items code="at0023">
        <items id="description">Kan sitte, men trenger mye hjelp ved forflytning.</items>
        <items id="text">Kan sitte, men trenger mye hjelp ved forflytning</items>
      </items>
      <items code="at0024">
        <items id="description">Klarer forflytning med litt hjelp eller tilsyn.</items>
        <items id="text">Klarer forflytning med litt hjelp eller tilsyn</items>
      </items>
      <items code="at0025">
        <items id="description">Klarer også å låse rullestol og bevege 
fotstøtte.</items>
        <items id="text">Selvhjulpen</items>
      </items>
      <items code="at0026">
        <items id="description">Evne til å ta til seg mat.</items>
        <items id="text">Spising</items>
      </items>
      <items code="at0027">
        <items id="description">Helt avhengig av hjelp.</items>
        <items id="text">Helt avhengig av hjelp</items>
      </items>
      <items code="at0028">
        <items id="description">Behov for noe hjelp, f.eks. til å skjære opp maten.</items>
        <items id="text">Behov for noe hjelp</items>
      </items>
      <items code="at0029">
        <items id="description">Helt selvhjulpen. Kan bruke nødvendige hjelpemidler 
og spiser innen rimelig tid.</items>
        <items id="text">Selvhjulpen</items>
      </items>
      <items code="at0030">
        <items id="description">Evne til å gå på toalettet.</items>
        <items id="text">Toalettbesøk</items>
      </items>
      <items code="at0031">
        <items id="description">Kan ikke bruke toalett.</items>
        <items id="text">Kan ikke bruke toalett</items>
      </items>
      <items code="at0032">
        <items id="description">Trenger hjelp til forflytning, klær, tørke seg.</items>
        <items id="text">Trenger hjelp</items>
      </items>
      <items code="at0033">
        <items id="description">Selvhjulpen ved toalettbesøk eller bruk av toalettstol. Ordner klær, tørker seg, spyler toalettet eller tømmer 
bekken.</items>
        <items id="text">Selvhjulpen</items>
      </items>
      <items code="at0034">
        <items id="description">Evne til å ivareta personlig hygiene med f.eks. tannpuss, barbering og kroppsvask.</items>
        <items id="text">Personlig hygiene</items>
      </items>
      <items code="at0035">
        <items id="description">Trenger hjelp til en eller flere oppgaver.</items>
        <items id="text">Trenger hjelp</items>
      </items>
      <items code="at0036">
        <items id="description">Selvhjulpen. Klarer å vaske ansiktet, kjemme håret, 
pusse tenner og barbere seg.</items>
        <items id="text">Selvhjulpen</items>
      </items>
      <items code="at0037">
        <items id="description">Helt inkontinent eller hyppige "uhell".</items>
        <items id="text">Helt inkontinent</items>
      </items>
      <items code="at0038">
        <items id="description">Klarer selv evt. å sette stikkpille/klyx.</items>
        <items id="text">Kontinent</items>
      </items>
      <items code="at0039">
        <items id="description">Klarer å gå 50 meter, kan bruke stokk eller krykker, men ikke annet ganghjelpemiddel.</items>
        <items id="text">Kan gå uten hjelp, evt med stokk/krykker</items>
      </items>
      <items code="at0040">
        <items id="description">Evne til å gå i trapper.</items>
        <items id="text">Trappegang</items>
      </items>
      <items code="at0041">
        <items id="description">Kan ikke gå i trapp.</items>
        <items id="text">Kan ikke gå i trapp</items>
      </items>
      <items code="at0042">
        <items id="description">Trenger hjelp/tilsyn av en person.</items>
        <items id="text">Trenger hjelp/tilsyn</items>
      </items>
      <items code="at0043">
        <items id="description">Kan evt. bruke ganghjelpemidler.</items>
        <items id="text">Selvhjulpen</items>
      </items>
      <items code="at0044">
        <items id="description">Nedsatt kontroll og enkelte "uhell" eller trenger hjelp til evt. å sette stikkpiller/klyx.</items>
        <items id="text">Nedsatt kontroll</items>
      </items>
    </term_definitions>
  </component_ontologies>
  <component_ontologies archetype_id="openEHR-EHR-OBSERVATION.stratify_no.v1">
    <term_definitions language="en">
      <items code="at0000">
        <items id="description">Assessment tool for identifying falls risk in elderly patients admitted to hospital or other 24h healthcare institutions. This version of the tool is based on a modified Norwegian translation, which has been deployed by the The Norwegian Patient Safety Programme.</items>
        <items id="text">STRATIFY Falls Risk Assessment Tool</items>
      </items>
      <items code="at0001">
        <items id="description">@ internal @</items>
        <items id="text">Event Series</items>
      </items>
      <items code="at0002">
        <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
        <items id="text">Any event</items>
      </items>
      <items code="at0003">
